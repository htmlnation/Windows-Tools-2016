      Case 6
      objShell.Run "C:\qL\shortcuts\e.lnk"
             Case 7
      objShell.Run "VNCInstall.hta"
      End Select
    End sub

    Sub cmd
    objShell.Sendkeys "runas /user:domain\Admin c:\windows\system32\cmd.exe"
    idTimer = window.setTimeout("PZ1", 100, "VBScript")
    End Sub

    Sub Adminmsccmd
      objShell.Sendkeys "runas /user:domain\Admin c:\windows\system32\cmd.exe"
      idTimer = window.setTimeout("PZ1admin", 500, "VBScript")
    End Sub

    Sub AdminMSC
      objShell.Sendkeys "runas /user:domain\Admin ""mmc c:\qL\C1.msc"""
      idTimer = window.setTimeout("PZ1admin", 500, "VBScript")
    End Sub
 
    Sub Admincomp
      objShell.Sendkeys "net use \\"
      objShell.Sendkeys BasicTextbox.Value
      objShell.Sendkeys "\c$ password /user:domain\Admin"
      objShell.Sendkeys "{ENTER}"
      idTimer = window.setTimeout("Admincomp2", 500, "VBScript") 
    End Sub
    
   Sub Admincomp2
      objShell.Sendkeys "start \\"
      objShell.Sendkeys BasicTextbox.Value
      objShell.Sendkeys "\c$" 
      objShell.Sendkeys "{ENTER}" 
      objShell.Sendkeys "exit"
      objShell.Sendkeys "{ENTER}"
    End Sub

    Sub Adminmscstart
      objShell.Sendkeys "mmc c:\qL\C1.msc"
      objShell.Sendkeys "{ENTER}"
      objShell.Sendkeys "exit"
      objShell.Sendkeys "{ENTER}"
    End Sub

    
    Sub PZ1
 objShell.Sendkeys "{ENTER}" 
 idTimer = window.setTimeout("PZ2", 500, "VBScript")
    End Sub
  
   
    Sub PZ1admin
 objShell.Sendkeys "{ENTER}" 
 idTimer = window.setTimeout("PZ2admin", 500, "VBScript")
    End Sub

    Sub PZ2
        objShell.Sendkeys "password"
 objShell.Sendkeys "{ENTER}"
 objShell.Sendkeys "exit"
 objShell.Sendkeys "{ENTER}"
    End Sub 

    Sub PZ2admin
        objShell.Sendkeys "password"
 objShell.Sendkeys "{ENTER}"
 objShell.Sendkeys "exit"
 objShell.Sendkeys "{ENTER}"
 <!--   idTimer = window.setTimeout("adminmscstart", 500, "VBScript")
 -->
    End Sub

    Sub ieopen
        For Each objButton in RadioOption
            If objButton.Checked Then
               Select Case objButton.Value
              Case 8
                     objShell.Sendkeys "start https://admin.securence.com/" 
                     objShell.Sendkeys "{ENTER}" 
                     objShell.Sendkeys "exit"
                     objShell.Sendkeys "{ENTER}"
             Case 9
                     objShell.Sendkeys "start https://www.ups.com/" 
                     objShell.Sendkeys "{ENTER}" 
                     objShell.Sendkeys "exit"
                     objShell.Sendkeys "{ENTER}"          
      Case 10
                     objShell.Sendkeys "start https://google.com" 
                     objShell.Sendkeys "{ENTER}" 
                     objShell.Sendkeys "exit"
                     objShell.Sendkeys "{ENTER}" 
      Case 11
                     objShell.Sendkeys "start https://google.com"
                     objShell.Sendkeys "{ENTER}" 
                     objShell.Sendkeys "exit"
                     objShell.Sendkeys "{ENTER}" 
       End Select     
            End If
        Next

      
    end Sub  

      
</SCRIPT>

