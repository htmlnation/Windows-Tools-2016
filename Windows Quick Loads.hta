    End Select
  End sub

   Sub tools

     Select Case Optiontools.Value
    
       <!-- tools -->
              Case 1
                     objShell.Run "cmd.exe"
       idTimer = window.setTimeout("AdminMSC", 500, "VBScript")
      Case 2
      objShell.Run "cmd.exe"
      idTimer = window.setTimeout("Adminmsccmd", 500, "VBScript")
      Case 3
             objShell.Run "info.hta"
      Case 4
      objShell.Run "C:\qL\shortcuts\6.lnk" 
      Case 5
